{
  "name": "ui-library/symfony-adapter",
  "description": "Symfony Adapter for UI Library",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/framework-bundle": "^6.0|^7.0",
    "twig/twig": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "UiLibrary\\SymfonyAdapter\\": "src/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.0|7.0"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UiLibrary\\SymfonyAdapter\\Tests\\": "tests/"
    }
  }
}
